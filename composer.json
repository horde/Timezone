{
    "name": "horde/timezone",
    "description": "Timezone library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Timezone",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        }
    ],
    "time": "2022-11-05",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/date": "^3 || dev-FRAMEWORK_6_0",
        "horde/icalendar": "^3 || dev-FRAMEWORK_6_0",
        "horde/vfs": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "suggest": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0"
    },
    "autoload": {
        "psr-0": {
            "Horde_Timezone": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Timezone\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    },
    "minimum-stability": "dev"
}
